<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true" >
        <shape>
            <solid
                android:color="@color/amarilloFadming" />
            <corners
                android:radius="@dimen/radius_boton_escanear" />
        </shape>
    </item>
    <item>
        <shape>
            <gradient
                android:startColor="@color/verdeMilitar"
                android:centerColor="@color/verdeFadming"
                android:endColor="@color/verdeMilitar"/>
            <corners
                android:radius="@dimen/radius_boton_escanear" />
        </shape>
    </item>
</selector>